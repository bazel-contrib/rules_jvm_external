module(name = "rules_jvm_external_example_java_export")

protobuf_version = "33.4"

bazel_dep(name = "protobuf", version = protobuf_version)
bazel_dep(name = "rules_jvm_external", version = "ignored")
bazel_dep(name = "rules_proto", version = "7.1.0")

local_path_override(
    module_name = "rules_jvm_external",
    path = "../../",
)

maven = use_extension("@rules_jvm_external//:extensions.bzl", "maven")
maven.install(
    artifacts = [
        "com.google.guava:guava:29.0-jre",
        "com.google.protobuf:protobuf-java:4.%s" % protobuf_version,
    ],
    fetch_sources = True,
    known_contributing_modules = ["protobuf"],
    lock_file = "//:maven_install.json",
)
use_repo(maven, "maven")
